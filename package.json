{
  "name": "cwg-progress",
  "description": "A raid progress tracker for the CWG (Christian WoW Guilds) community",
  "version": "0.2.0",
  "private": true,
  "homepage": "./cwg-progress",
  "author": {
    "name": "Charmy Rosewolf",
    "email": "CharmyRosewolf@gmail.com"
  },
  "contributors": [],
  "scripts": {
    "dev": "next dev",
    "clear-cache": "node -e \"require('fs').rmSync('.next/cache/dev-api',{recursive:true,force:true})\" && echo Dev cache cleared",
    "build": "next build",
    "start": "next start",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src/",
    "format:staged": "prettier --write --tab-width 2 --single-quote --jsx-single-quote --trailing-comma none $(git diff --cached --name-only)"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.0.0",
    "@emotion/react": "^11.14.0",
    "@next/env": "16.1.6",
    "date-fns": "^3.6.0",
    "next": "16.1.6",
    "next-themes": "^0.4.6",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "typescript": "^5",
    "vite-tsconfig-paths": "^6.1.1",
    "vitest": "^4.0.18"
  },
  "overrides": {
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3"
  }
}
